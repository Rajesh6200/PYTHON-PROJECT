def romanToInt(s):
    roman = {'I': 1, 'V': 5, 'X': 10, 'L': 50, 'C': 100, 'D': 500, 'M': 1000, 'IV': 4, 'IX': 9, 'XL': 40, 'XC': 90,
             'CD': 400, 'CM': 900}
    i = 0
    num = 0
    while i < len(s):
        if i + 1 < len(s) and s[i:i + 3] in roman:
            num += roman[s[i:i + 3]]
            i += 3
        else:
            num += roman[s[i]]
            i += 1
    return num


x = input("Enter an number in Roman Numeral: ")
print("Value in decimal is ", romanToInt(x))
